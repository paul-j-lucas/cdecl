EXPECTED_EXIT=0
cdecl -bE <<END
#define t(x,y,z)  x ## y ## z

expand t(1,2,3)
expand t(,4,5)
expand t(6,,7)
expand t(8,9,)
expand t(10,,)
expand t(,11,)
expand t(,,12)
expand t(,,)
END
