#! /usr/bin/env expect

#exp_internal 1
spawn cdecl --color=never

expect "cdecl> "
# set options
send "set o\t\r"
expect default { exit 1 } -ex { using}

expect "cdecl> "
# set alt-tokens
send "set a\t; set\r"
expect default { exit 1 } -ex { alt-tokens}

expect "cdecl> "
# set nousing
send "set nou\t; set\r"
expect default { exit 1 } -ex { nousing}

# vim:set et sw=2 ts=2:
